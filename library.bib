pages = {1369--1375},
publisher = {Elsevier Ltd and IIR},
title = {{A comparison of different generalised modelling approaches for a scroll refrigerant compressor}},
url = {http://dx.doi.org/10.1016/j.ijrefrig.2013.01.002},
volume = {36},
year = {2013}
}
@article{Negrao2011,
