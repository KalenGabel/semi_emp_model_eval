volume = {59},
year = {2015}
}
@article{Duprez2010,
abstract = {This paper presents an improvement of the scroll compressors model previously published by Duprez et al. (2007). This improved model allows the calculation of refrigerant mass flow rate, power consumption and heat flow rate that would be released at the condenser of a heat pump equipped with the compressor, from the knowledge of operating conditions and parameters. Both basic and improved models have been tested on scroll compressors using different refrigerants. This study has been limited to compressors with a maximum electrical power of 14 kW and for evaporation temperatures ranging from -40 to 15 °C and condensation temperatures from 10 to 75 °C. The average discrepancies on mass flow rate, power consumption and heat flow rate are respectively 0.50%, 0.93% and 3.49%. Using a global parameter determination (based on several refrigerants data), this model can predict the behavior of a compressor with another fluid for which no manufacturer data are available. {\textcopyright} 2010 Elsevier Ltd and IIR.},
