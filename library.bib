journal = {Proceedings of the IEEE Conference on Decision and Control},
keywords = {adaptivity,online learning,stochastic convex optimization,subgradient methods},
pages = {5442--5444},
title = {{Adaptive Subgradient Methods for Online Learning and Stochastic Optimization}},
volume = {12},
year = {2012}
}
@inproceedings{Popovic1995a,
abstract = {A simple semi-empirical compressor model has been developed to be used in refrigeration system simulations. The model is based on thermodynamics principles and a large data base. The purpose of the model is to reduce the amount of data required to completely characterize compressor performance using alternative refrigerants. Two compressor performance parameters were defined. An effective pressure drop accounts for mass flow losses in the compressor, while numerically determined functional dependence of the heat transfer loss coefficient is related to compressor energy losses. These parameters depict compressor operation with reasonable accuracy. A total of 122 data points, 8 different refrigerants, and 2 compressors were utilized in the project. The model predicted mass flow rates and required compressor power for all data points within 10% of relative error.},
author = {Popovic, Predrag and Shapiro, Howard N.},
booktitle = {ASHRAE Transactions},
file = {:C\:/Users/kaleng/AppData/Local/Mendeley Ltd/Mendeley Desktop/Downloaded/A semi-empirical method for modeling a reciprocating compressor in refrigeration systems.pdf:pdf},
issn = {00012505},
number = {Pt 2},
pages = {367--382},
publisher = {ASHRAE},
title = {{Semi-empirical method for modeling a reciprocating compressor in refrigeration systems}},
volume = {101},
year = {1995}
}
@article{Mathison2008,
abstract = {This paper presents the development and validation of a computer model for hermetic two-stage rotary compressors that can be used for design optimization. The model considers the effects of refrigerant leakage between the compressor chambers, heat transfer between the cylinder wall and the refrigerant gas, and heat loss by natural convection to the surroundings. The model numerically solves mass and energy balances for each of the compressor's chambers to predict the pressure and temperature variations over a crankshaft revolution. The compressor power input and refrigerant mass flow rate are calculated and used to determine the isentropic and volumetric efficiencies. The model can operate both with and without intercooling between the stages to allow for study of the effects of intercooling on compressor performance. The model results are compared to the results of external performance tests that were conducted using a prototype compressor. The intermediate temperature and pressure, discharge temperature, power consumption, and mass flow rate were recorded for 17 different operating conditions. The model predicts the compressor power consumption and mass flow rate within ±5% of the experimental results. {\textcopyright} 2008 American Society of Heating, Refrigerating and Air-Conditioning Engineers, Inc.},
author = {Mathison, Margaret M. and Braun, James E. and Groll, Eckhard A.},
doi = {10.1080/10789669.2008.10391036},
file = {:C\:/Users/kaleng/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mathison, Braun, Groll - 2008 - Modeling of a Two-Stage rotary compressor.pdf:pdf},
issn = {10789669},
journal = {HVAC and R Research},
number = {5},
pages = {719--748},
title = {{Modeling of a Two-Stage rotary compressor}},
volume = {14},
year = {2008}
}
