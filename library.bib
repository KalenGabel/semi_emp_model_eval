issn = {13594311},
journal = {Applied Thermal Engineering},
keywords = {High pressure,Modelling,Scroll compressor,Testing},
number = {6-7},
pages = {605--615},
publisher = {Elsevier Ltd},
title = {{Characterization of a scroll compressor under extended operating conditions}},
